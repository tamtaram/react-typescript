{
    "extends": "salt-typescript",
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never",
          "": "never"
        }
      ]
    }
  }